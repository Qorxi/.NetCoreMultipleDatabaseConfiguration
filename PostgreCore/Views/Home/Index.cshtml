@model IEnumerable<PostgreCore.Models.ProdSearchable>
@{
    ViewData["Title"] = "Index";
}
<h1>Index</h1>
<a asp-controller="Home" asp-action="Create">Create new Employee</a>

<table class="table table-hover table-striped">
    <thead>
        <tr>
            <td> Id </td>
            <td> Name </td>
            <td> Age </td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.SearchColumn
                </td>
            </tr>
        }
    </tbody>
</table>

